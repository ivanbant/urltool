    "concurrently": "^8.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "mysql2": "^3.6.2",
    "nanoid": "^4.0.2",
    "qrcode": "^1.5.3",
    "qs": "^6.12.1",
    "react-icons": "^4.11.0",
    "react-toastify": "^9.1.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
